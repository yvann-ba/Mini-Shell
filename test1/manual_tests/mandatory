#DONE Prints without a new line
#
echo -n hello

#DONE update env variable value
export HELLO=123
export HELLO="beautiful world"

#DONE(pas sur) keep track of OLDPWD
cd obj
echo $PWD $OLDPWD

#DONE invalid command, followed by empty input, should keep the exit code
doesntexist

echo $?

#PAS BON invalid command, followed by empty variable, should clear the exit code
doesntexist
$EMPTY
echo $?

# DONE Check if there isn't a zombie process called `cat`
echo "hi" | cat | cat | cat | cat | cat | cat | cat
ps -a

# DONE Should skip the empty argument, and print hello after spaces
echo - "" "  " hello

# DONE Neither of these unsets should break the shell, and you should still be able to call `/bin/ls`
unset USER
unset PATH
unset PWD
/bin/ls

# DONE This should not change the current directory
cd .. hi

# DONE Empty `cd` moves to home
cd
